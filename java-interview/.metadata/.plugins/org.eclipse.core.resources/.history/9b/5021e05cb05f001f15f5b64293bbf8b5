package com.app.code;

public class Swapping2Numbers {

	public static void main(String[] args) {
 
		int a=10, b=20;
		System.out.println("before swapping values are..."+a+" "+b);
		// logic using temp variable ie third variable
		int temp =a;
		a=b;
		b=temp;
		
		System.out.println("before swapping values are..."+a+" "+b);

	}

}
