package com.app.pattern;

public class ReverseHillPattern {

	public static void main(String[] args) {
 
		int n=5;
		for(int i=1;i<=n;i++)
		{
			for (int j=1;j<=i;j++)
			{
				System.out.println(" ");
			}

			for (int j=1;j<=i;j++)
			{
				System.out.println("*");
			}
		}
	}

}
/*
incresing space 
decreasing star
decreasing star

*/